<AnchorPane prefHeight="263.0" prefWidth="416.0" xmlns="http://javafx.com/javafx/23"
            xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox prefHeight="263.0" prefWidth="416.0">
            <children>
                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="37.0" prefWidth="469.0" text="Login Page">
                    <font>
                        <Font size="41.0"/>
                    </font>
                </Label>
                <GridPane prefWidth="451.0" vgap="15.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="221.0" minWidth="10.0" prefWidth="158.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="328.0" minWidth="10.0" prefWidth="218.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <Label alignment="CENTER_RIGHT" prefHeight="17.0" prefWidth="132.0" text="Username">
                            <GridPane.margin>
                                <Insets right="20.0"/>
                            </GridPane.margin>
                        </Label>
                        <Label alignment="CENTER_RIGHT" prefHeight="17.0" prefWidth="128.0" text="Password"
                               GridPane.rowIndex="1">
                            <GridPane.margin>
                                <Insets right="20.0"/>
                            </GridPane.margin>
                        </Label>
                        <TextField fx:id="usernameTextField" prefHeight="26.0" prefWidth="224.0"
