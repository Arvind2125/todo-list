                                prefWidth="81.0" text="Login" GridPane.columnIndex="1" GridPane.halignment="RIGHT"
                                GridPane.rowIndex="2"/>
                        <PasswordField fx:id="passwordTextField" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                    </children>
                    <VBox.margin>
                        <Insets top="30.0"/>
                    </VBox.margin>
                    <padding>
                        <Insets right="20.0"/>
                    </padding>
                </GridPane>
            </children>
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
            </padding>
        </VBox>
